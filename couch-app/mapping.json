{
    "mappings": {
        "tumblr": {
            "properties": {
                "site": {"type": "string", "index": "not_analyzed"},
                "date": {"type": "date", "format":"YYYY-MM-dd HH:mm:ss z"},
                "favorite": {"type": "boolean"},
                "timestamp": {"type": "date"},
                "images": {
                    "index_name": "image",
                    "properties": {
                        "width": {"type": "integer"},
                        "fileno": {"type": "integer"},
                        "maxWidth": {"type": "integer"}
                    }
                }
            }
        }
    }
}
